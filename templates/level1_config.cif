;l.dep_set_id = d.dep_set_id and l.annotator_initials in ('%(initials)s', 'EBI') and 
( ( ( l.dep_status_code in ( 'AUTH', 'REFI', 'WAIT' ) and l.pdb_id != '?' ) or 
    ( l.pdb_id = '?' and l.dep_emdb_id != '?' and l.dep_status_code_emdb in ( 'AUTH', 'REFI', 'WAIT' ) ) )
  or 
  ( ( ( l.dep_status_code in ( 'HOLD', 'HPUB', 'OBS', 'REL', 'WDRN' ) and l.pdb_id != '?' ) or 
      ( l.pdb_id = '?' and l.dep_emdb_id != '?' and l.dep_status_code_emdb in ( 'HOLD', 'HPUB', 'OBS', 'REL', 'WDRN' ) ) ) 
    and 
    l.dep_notify <> '' )
  or
  ( l.dep_status_code in ('REL') and l.pdb_id != '?' and l.dep_post_rel_status not in ('?', '.'))
) 
order by dep_initial_deposition_date ASC
