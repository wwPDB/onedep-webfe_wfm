common_page_header_tmplt  script
;<script type='text/javascript' src='/wfm/js/depui.js'></script>

<script type="text/javascript">
var selected_tab_id = '%(firstTab)s';
var session_ID = '%(sessionid)s';
var annotator  = '%(annotator)s';
var status_map = { 'AUCO':9, 'AUTH':8, 'HOLD':5, 'HPUB':4, 'OBS':2, 'POLC':11, 'PROC':10, 'REFI':6, 'REL':1, 'REPL':12, 'WAIT':7, 'WDRN':3 };
var table_id_map = %(table_id_map)s;
var entry_count_map = %(entry_count_map)s;

var enum_map = { 'entry_by_initial'   : %(entry_by_initial)s,
                 'entry_by_sg_center' : %(entry_by_sg_center)s,
                 'entry_by_status'    : [['',''],['AUCO','AUCO'],['AUTH','AUTH'],['HOLD','HOLD'],['HPUB','HPUB'],['OBS','OBS'],['POLC','POLC'],['PROC','PROC'],['REFI','REFI'],['REL','REL'],['REPL','REPL'],['WAIT','WAIT'],['WDRN','WDRN']]
};

var search_label_map = { 'entry_by_ids'       : [ 'table_1',  '<b>Dep/PDB/BMRB/EMDB ID(s):</b> &nbsp; ',  'Find Entry by Dep/PDB/BMRB/EMDB ID(s)'     ],
                         'entry_by_initial'   : [ 'table_1',  '<b>Annotator Initials:</b> &nbsp; ',       'Find Entry by Annotator Initials'          ],
                         'entry_by_status'    : [ 'table_1',  '<b>Status:</b> &nbsp; ',                   'Find Entry by Status'                      ],
                         'entry_by_sg_center' : [ 'table_1',  '<b>SG Centers:</b> &nbsp; ',               'Find Entry by SG Centers'                  ],
                         'entry_by_hold_date' : [ 'table_1',  '',                                         ''                                          ],
                         'entry_by_wfm_unlock' : [ 'table_1',  '',                                         ''                                         ],
                         'entry_by_title'     : [ 'table_1',  '<b>Title:</b> &nbsp; ',                    'Find Entry by Title'                       ],
                         'entry_by_author'    : [ 'table_1',  '<b>Author:</b> &nbsp; ',                   'Find Entry by Author'                      ],
                         'group_by_ids'       : [ 'table_4',  '<b>group ID(s):</b> &nbsp; ',              'Find Group by Group ID(s)'                 ],
                         'user_by_ids'        : [ 'table_2',  '<b>Dep/PDB/BMRB/EMDB ID(s):</b> &nbsp; ',  'Find Depositor by Dep/PDB/BMRB/EMDB ID(s)' ],
                         'user_by_lastname'   : [ 'table_2',  '<b>Last Name:</b> &nbsp; ',                'Find Depositor by Last Name'               ],
                         'user_by_email'      : [ 'table_2',  '<b>Email:</b> &nbsp; ',                    'Find Depositor by E-Mail'                  ],
                         'dep_by_ids'         : [ 'table_3',  '<b>Dep ID(s):</b> &nbsp; ',                'Find On-going Deposition by Dep ID(s)'     ],
                         'dep_by_title'       : [ 'table_3',  '<b>Title:</b> &nbsp; ',                    'Find On-going Deposition by Title'         ],
                         'dep_by_author'      : [ 'table_3',  '<b>Author:</b> &nbsp; ',                   'Find On-going Deposition by Author'        ]
};

function statusSorter(status_a, status_b) {
       var a = status_map[status_a];
       var b = status_map[status_b];
       if (a < b) return -1;
       if (a > b) return 1;
       return 0;
}

function defaultSorter(a, b) {
       var int_a = parseInt(a);
       var int_b = parseInt(b);
       if (int_a < int_b) return -1;
       if (int_a > int_b) return 1;
       return 0;
}

function rowStyle(row, index) {
    var val = row['auxiliary'];
    if (val != '') return { classes: val };
    return {};
}

function cellStyle(value, row, index) {
    if (value != '') return { classes: 'cell-font-color-red' }
    return {};
}
</script>

