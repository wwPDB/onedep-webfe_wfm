 and 
 ( 
   ( l.dep_status_code in ( 'REPL', 'AUCO', 'REUP', 'AUXS', 'AUXU' ) and l.pdb_id != '?' ) or
   ( l.pdb_id = '?' and l.dep_emdb_id != '?' and l.dep_status_code_emdb in ( 'REPL', 'AUCO', 'REUP', 'AUXS', 'AUXU' ) ) or
   ( l.pdb_id != '?' and l.dep_status_code = 'REL' and l.dep_post_rel_status in ( 'REPL', 'AUCO', 'REUP', 'AUXS', 'AUXU' ) )
 )
